library(MASS)
library(leaps)
library(car)
setwd('F:\\dev\\lcc\\R-models')
master<-read.csv('master.csv')
#Examine structure
str(master)
############################
# STEPWISE
############################
#See:http://www.statmethods.net/stats/regression.html
#TODO:  ADD CREW CAPACITY--Make DICHATOMOUS
wbsModel_turret<- lm(WBS_TURRET_ASSEMBLY~GW+WIDTH+ENGINE_PWR_HP+WPN_CALIBER_MM+TRACK_TIRE, data=master)
wbsModel_turret.step<- stepAIC(wbsModel_turret, direction="both")
wbsModel_turret.step$anova
summary(wbsModel_turret.step)
############################
# LEAP
############################
